{
    "version": 2,
    "buildCommand": "cd frontend && npm install && npm run build:vercel",
    "devCommand": "cd frontend && npm start",
    "installCommand": "cd frontend && npm install",
    "outputDirectory": "frontend/dist/qafront",
    "functions": {
        "api/index.py": {
            "maxDuration": 60
        }
    },
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/dist/qafront/$1"
        }
    ]
}